    "@typescript-eslint/parser": "^8.15.0",
    "autoprefixer": "^10.4.19",
    "concurrently": "^9.0.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.18",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.5",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "seedrandom": "^3.0.5",
    "tailwindcss": "^3.4.13",
    "ts-node": "^10.9.2",
    "tsx": "^4.15.7",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.15.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ],
    "*.{ts,tsx,js,jsx,md,css,json}": [
